{
    "name": "ecs.service.cpuutilization: ${service_name}",
    "type": "metric alert",
    "query": "avg(${ecs_cpu_utilization_timeframe}):avg:aws.ecs.service.cpuutilization{servicename:${service_name}} by {servicename} ${ecs_cpu_utilization_operator} ${ecs_cpu_utilization_critical_threshold}",
    "message": "ecs service cpuutilization event monitor triggered for ${service_name}. Notify: ${notification_targets}",
    "tags": [],
    "options": {
        "notify_audit": false,
        "locked": false,
        "timeout_h": 0,
        "require_full_window": false,
        "notify_no_data": false,
        "renotify_interval": "0",
        "evaluation_delay": 900,
        "thresholds": {
            "critical": "${ecs_cpu_utilization_critical_threshold}"
        }
    },
    "priority": null
}