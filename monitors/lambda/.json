{
	"name": "lambda.unreserved_concurrent_executions.maximum: ${function_name}",
	"type": "query alert",
	"query": "sum(${lambda.unreserved_concurrent_executions.maximum_timeframe}):sum:aws.lambda.unreserved_concurrent_executions.maximum{functionname:${function_name}} ${lambda.unreserved_concurrent_executions.maximum_operator} ${lambda.unreserved_concurrent_executions.maximum_critical}",
	"message": "lambda.unreserved_concurrent_executions.maximum monitor triggered for ${function_name}. Notify: ${notification_targets}",
	"tags": [],
	"options": {
		"thresholds": {
			"critical": "${lambda.unreserved_concurrent_executions.maximum_critical}"
		},
		"notify_audit": false,
		"require_full_window": false,
		"notify_no_data": false,
		"renotify_interval": 0,
		"include_tags": false,
		"evaluation_delay": 900
	}
}